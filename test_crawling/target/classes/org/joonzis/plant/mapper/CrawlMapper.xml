<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.joonzis.plant.mapper.CrawlMapper">
	<select id="searchPlants" resultType="String">
		SELECT * FROM search_plant_name
	</select>
	<insert id="insertSearchPlantNames">
		INSERT INTO tbl_plant_names VALUES(#{plant_name})
	</insert>
</mapper>
